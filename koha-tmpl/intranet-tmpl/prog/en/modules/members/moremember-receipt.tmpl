<div id="receipt">
<B>Checkout Receipt</b><br>
<b><!-- TMPL_VAR name="LibraryName" --></b>

<SCRIPT LANGUAGE="JavaScript">
Stamp = new Date();
year = Stamp.getYear();
if (year < 2000) year = 1900 + year;
document.write('<B>' + (Stamp.getMonth() + 1) +"/"+Stamp.getDate()+ "/"+ year + '</B>&nbsp;&nbsp;');
var Hours;
var Mins;
var Time;
Hours = Stamp.getHours();
if (Hours >= 12) {
Time = " P.M.";
}
else {
Time = " A.M.";
}
if (Hours > 12) {
Hours -= 12;
}
if (Hours == 0) {
Hours = 12;
}
Mins = Stamp.getMinutes();
if (Mins < 10) {
Mins = "0" + Mins;
}	
document.write('<B>' + Hours + ":" + Mins + Time + '</B>');
</SCRIPT>
